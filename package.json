    "marked": "0.3.6",
    "ng2-bootstrap": "1.4.2",
    "ng2-page-scroll": "4.0.0-beta.4",
    "ngm-cli": "0.4.4",
    "npm-run-all": "4.0.2",
    "prismjs-loader": "0.0.4",
    "protractor": "5.1.1",
    "reflect-metadata": "0.1.10",
    "require-dir": "0.3.1",
    "rxjs": "5.2.0",
    "ts-helpers": "^1.1.1",
    "tslint": "4.5.1",
    "tslint-config-valorsoft": "1.2.0",
    "typedoc": "0.5.7",
    "typescript": "2.0.10",
    "wallaby-webpack": "0.0.34",
    "webdriver-manager": "12.0.2",
    "webpack-bundle-analyzer": "2.3.0",
    "zone.js": "0.7.7"
  },
  "contributors": [
    {
      "name": "Dmitriy Shekhovtsov",
