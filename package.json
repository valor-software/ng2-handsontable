{
  "name": "ng2-handsontable",
  "version": "0.47.0",
  "description": "angular2 handsontable components",
  "scripts": {
    "flow.install:typings": "./node_modules/.bin/typings install",
    "flow.compile": "npm run flow.install:typings && npm run flow.compile:common && npm run flow.compile:system ",
    "flow.compile:common": "./node_modules/.bin/tsc",
    "flow.compile:system": "./.config/bundle-system.js",
    "flow.deploy:gh-pages": "npm run flow.build:prod && ./node_modules/.bin/gh-pages -d demo-build",
    "flow.build:prod": "NODE_ENV=production ./node_modules/.bin/webpack --progress --color",
    "flow.eslint": "./node_modules/.bin/eslint --ignore-path .gitignore  --ext js --fix . .config",
    "flow.tslint": "./node_modules/.bin/gulp lint",
    "flow.lint": "npm run flow.eslint && npm run flow.tslint",
    "prestart": "npm install",
    "flow.clean": "./node_modules/.bin/del bundles coverage demo-build typings \"components/**/*.+(js|d.ts|js.map)\" dist \"ng2-bootstrap.+(js|d.ts|js.map)\"",
    "flow.serve:dev": "./node_modules/.bin/webpack-dev-server --hot --inline --colors --display-error-details --display-cached",
    "prepublish": "npm run flow.clean && npm run flow.compile",
    "postpublish": "npm run flow.deploy:gh-pages",
    "start": "npm run flow.serve:dev",
    "pretest": "npm run flow.lint"
  },
  "main": "dist/index.js",
  "keywords": [
    "angular2",
    "bootstrap",
    "angularjs",
    "twitter-bootstrap"
  ],
  "author": "Vyacheslav Chub <vyacheslav.chub@valor-software.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/valor-software/ng2-handsontable.git"
  },
  "bugs": {
    "url": "https://github.com/valor-software/ng2-handsontable/issues"
  },
  "homepage": "https://github.com/valor-software/ng2-handsontable#readme",
  "dependencies": {
    "angular2": "2.0.0-beta.15",
    "moment": "^2.13.0"
  },
  "peerDependencies": {
    "angular2": "2.0.0-beta.15"
  },
  "devDependencies": {
    "async": "1.5.2",
    "balanced-match": "^0.4.0",
    "bootstrap": "3.3.6",
    "chroma-js": "^1.1.1",
    "clean-webpack-plugin": "0.1.8",
    "compression-webpack-plugin": "0.3.1",
    "copy-webpack-plugin": "^2.1.1",
    "cpy-cli": "^1.0.0",
    "del": "^2.2.0",
    "del-cli": "^0.2.0",
    "es6-promise": "^3.1.2",
    "es6-shim": "0.35.0",
    "es7-reflect-metadata": "^1.6.0",
    "eslint": "2.7.0",
    "eslint-config-valorsoft": "0.0.10",
    "exports-loader": "0.6.3",
    "file-loader": "0.8.5",
    "gulp": "3.9.1",
    "gulp-clean": "0.3.2",
    "gulp-eslint": "2.0.0",
    "gulp-size": "2.1.0",
    "gulp-tslint": "4.3.5",
    "handsontable": "^0.24.2",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "^2.16.0",
    "markdown-loader": "0.1.7",
    "marked": "0.3.5",
    "ng2-bootstrap": "^1.0.13",
    "pre-commit": "1.1.2",
    "prismjs": "valorkin/prism",
    "prismjs-loader": "0.0.2",
    "raw-loader": "0.5.1",
    "reflect-metadata": "0.1.2",
    "require-dir": "0.3.0",
    "ruleJS": "https://github.com/handsontable/ruleJS",
    "rxjs": "5.0.0-beta.2",
    "source-map-loader": "^0.1.5",
    "systemjs-builder": "0.15.15",
    "ts-loader": "0.8.2",
    "tslint-config-valorsoft": "0.0.6",
    "typescript": "1.8.10",
    "typings": "^0.8.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "1.14.1",
    "zone.js": "^0.6.12"
  }
}
