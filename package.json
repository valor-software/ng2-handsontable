    "exports-loader": "0.6.3",
    "file-loader": "0.9.0",
    "gulp": "3.9.1",
    "gulp-clean": "0.3.2",
    "gulp-eslint": "3.0.1",
    "gulp-size": "2.1.0",
    "gulp-tsc": "1.2.0",
    "gulp-tslint": "6.0.1",
    "html-loader": "0.4.3",
    "markdown-loader": "0.1.7",
