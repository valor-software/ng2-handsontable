{
  "name": "ng2-handsontable",
  "version": "0.46.0",
  "description": "angular2 handsontable components",
  "scripts": {
    "deploy": "NODE_ENV=production webpack -p --progress --color --optimize-minimize --optimize-dedupe --optimize-occurence-order",
    "prepublish": "gulp clean && tsc",
    "prestart": "npm install",
    "server": "webpack-dev-server --hot --inline --colors --display-error-details --display-cached",
    "start": "npm run server",
    "test": "gulp lint"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "components"
  ],
  "keywords": [
    "angular2",
    "bootstrap",
    "angularjs",
    "twitter-bootstrap"
  ],
  "author": "Vyacheslav Chub <vyacheslav.chub@valor-software.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/valor-software/ng2-handsontable.git"
  },
  "bugs": {
    "url": "https://github.com/valor-software/ng2-handsontable/issues"
  },
  "homepage": "https://github.com/valor-software/ng2-handsontable#readme",
  "dependencies": {
    "angular2": "2.0.0-beta.15",
    "reflect-metadata": "0.1.2",
    "ts-loader": "0.8.2",
    "zone.js": "0.6.11"
  },
  "devDependencies": {
    "bootstrap": "3.3.6",
    "ng2-bootstrap": "1.0.12",
    "clean-webpack-plugin": "0.1.8",
    "compression-webpack-plugin": "0.3.1",
    "es6-shim": "0.35.0",
    "eslint": "2.7.0",
    "exports-loader": "0.6.3",
    "file-loader": "0.8.5",
    "gulp": "3.9.1",
    "gulp-clean": "0.3.2",
    "gulp-eslint": "2.0.0",
    "gulp-size": "2.1.0",
    "gulp-tsc": "1.1.5",
    "gulp-tslint": "4.3.5",
    "html-loader": "0.4.3",
    "markdown-loader": "0.1.7",
    "marked": "0.3.5",
    "moment": "2.12.0",
    "pre-commit": "1.1.2",
    "prismjs": "valorkin/prism",
    "prismjs-loader": "0.0.2",
    "raw-loader": "0.5.1",
    "require-dir": "0.3.0",
    "rxjs": "5.0.0-beta.2",
    "typescript": "1.8.10",
    "webpack": "1.12.15",
    "webpack-dev-server": "1.14.1"
  }
}
