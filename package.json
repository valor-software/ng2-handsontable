{
  "name": "ng2-handsontable-base",
  "version": "3.0.0",
  "description": "Angular4 Handsontable components",
  "private": true,
  "scripts": {
    "lite-server": "lite-server -c demo/bs-config.js",
    "typedoc": "typedoc --exclude '**/*.spec.ts' ./src/",
    "demo.docs": "node scripts/docs/get-doc.js",
    "demo.serve": "run-s build link demo.build lite-server",
    "demo.gh-pages": "run-s build demo.build demo.deploy",
    "demo.build": "ng build -prod --aot=false",
    "demo.deploy": "gh-pages -d demo/dist",
    "demo-beta-deploy": "gh-pages -d demo/dist -r git@github.com:valorkin/ng2-handsontable.git -b gh-pages",
    "link": "ngm link -p src --here",
    "lint": "exit 0",
    "disable-lint": "tslint \"**/*.ts\" -c tslint.json --fix --type-check -t prose -e \"node_modules/**\" -e \"dist/**\" -e \"temp/**\" -e \"scripts/docs/**\"",
    "flow.changelog": "conventional-changelog -i CHANGELOG.md -s -p angular",
    "flow.github-release": "conventional-github-releaser -p angular",
    "build": "ngm build -p src --clean",
    "build.watch": "ngm build -p src --watch --skip-bundles",
    "start": "ng serve --host 0.0.0.0",
    "pretest": "run-s lint build link",
    "test": "ng test -sr",
    "test-cross": "SAUCE=true ng test -sr",
    "test-coverage": "ng test -sr -cc",
    "test-watch": "npm run pretest && ng test -w",
    "postversion": "run-s changelog git-add git-commit-changelog",
    "git-add": "git add -A",
    "git-commit-changelog": "git commit -am \"chore(changelog): update\"",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -p angular -r 2",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "e2e-cross": "SAUCE=true npm run e2e"
  },
  "main": "bundles/ng2-handsontable.umd.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "keywords": [
    "angular",
    "angular2",
    "angular4",
    "handsontable",
    "table"
  ],
  "author": "Vyacheslav Chub <vyacheslav.chub@valor-software.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/valor-software/ng2-handsontable.git"
  },
  "bugs": {
    "url": "https://github.com/valor-software/ng2-handsontable/issues"
  },
  "homepage": "https://github.com/valor-software/ng2-handsontable#readme",
  "dependencies": {
    "handsontable": ">= 0.37 < 1.0"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.1",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/language-service": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@angular/tsc-wrapped": "4.1.3",
    "@types/jasmine": "2.5.48",
    "@types/marked": "0.0.28",
    "@types/node": "7.0.0",
    "bootstrap": "3.3.7",
    "chroma-js": "1.3.4",
    "classlist-polyfill": "1.2.0",
    "codecov": "2.2.0",
    "codelyzer": "3.0.1",
    "conventional-changelog-cli": "1.3.1",
    "conventional-github-releaser": "1.1.9",
    "core-js": "^2.4.1",
    "cpy": "5.0.0",
    "cpy-cli": "1.0.1",
    "del-cli": "1.0.0",
    "gh-pages": "1.0.0",
    "gitignore-to-glob": "0.3.0",
    "google-code-prettify": "1.0.5",
    "html-loader": "0.4.5",
    "jasmine": "2.6.0",
    "jasmine-core": "2.6.2",
    "jasmine-data-provider": "2.2.0",
    "jasmine-spec-reporter": "4.1.0",
    "karma": "1.7.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-remap-istanbul": "0.6.0",
    "karma-sauce-launcher": "1.1.0",
    "karma-spec-reporter": "0.0.31",
    "lite-server": "2.3.0",
    "lodash": "4.17.4",
    "markdown-loader": "2.0.2",
    "marked": "0.3.6",
    "ngm-cli": "0.5.2",
    "ngx-bootstrap": "1.6.6",
    "npm-run-all": "4.0.2",
    "prismjs-loader": "0.0.4",
    "protractor": "5.1.2",
    "reflect-metadata": "0.1.10",
    "require-dir": "0.3.2",
    "rxjs": "5.4.0",
    "ts-helpers": "^1.1.1",
    "tslint": "5.4.0",
    "tslint-config-valorsoft": "2.0.1",
    "typedoc": "0.7.1",
    "typescript": "2.3.4",
    "wallaby-webpack": "0.0.38",
    "webdriver-manager": "12.0.6",
    "webpack-bundle-analyzer": "2.8.2",
    "zone.js": "0.8.20"
  },
  "contributors": [
    {
      "name": "Dmitriy Shekhovtsov",
      "email": "valorkin@gmail.com",
      "url": "https://github.com/valorkin"
    },
    {
      "name": "Rene Hamburger",
      "email": "rene.hamburger@leanix.net",
      "url": "https://github.com/renehamburger"
    }
  ]
}
